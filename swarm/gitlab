docker run -d \
  --hostname gitlab.londres.casa \
  -p 44388:443 \
  -p 8189:80 \
  -p 2288:22 \
  --name gitlab \
  --restart always \
  -v /storage/londres/global/config/gitlab:/etc/gitlab \
  -v /storage/londres/global/data/gitlab/logs:/var/log/gitlab \
  -v /storage/londres/global/data/gitlab/data:/var/opt/gitlab \
  --label traefik.backend=gitlab \
  --label traefik.port=80 \
  --label traefik.frontend.rule="Host:gitlab.londres.casa" \
  --label traefik.frontend.entryPoints=http \
  ulm0/gitlab


docker run  -it -d \
    --name cloud9 \
    -p 8181:8181 \
    -v openhab_conf:/workspace/ \
    --label traefik.backend=cloud9 \
    --label traefik.port=8181 \
    --label traefik.frontend.rule="Host:cloud9.londres.casa" \
    --label traefik.frontend.entryPoints=http \
    hwegge2/rpi-cloud9-ide node server.js -w/workspace --listen 0.0.0.0 -a :
  
